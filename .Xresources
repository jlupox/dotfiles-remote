URxvt*background: black
URxvt*foreground: gray90
URxvt*cursorColor: wheat

*color7: gray90
*color12: #80a0ff

*VT100.modifyCursorKeys:0

URxvt*geometry: 84x24
URxvt*font: \
  xft:Liberation Mono:pixelsize=13.5, \
  xft:DejaVu Sans Mono:pixelsize=13.5, \
  xft:Baekmuk Dotum
URxvt*boldFont: \
  xft:Liberation Mono:style=Bold:pixelsize=13.5, \
  xft:DejaVu Sans Mono:style=Bold:pixelsize=13.5, \
  xft:Baekmuk Dotum:style=Bold
URxvt*saveLines: 8192

URxvt*scrollBar: false
URxvt*cutchars: "()*,<>[]{}|'
URxvt*visualBell: false

URxvt*perl-ext-common: default,matcher
URxvt*urlLauncher: /usr/bin/firefox
URxvt*colorUL: #86a2be

URxvt*perl-lib: /home/jorda/.urxvt/lib
URxvt*perl-ext: cwd-spawn,keyboard-select

URxvt*keysym.F3: perl:matcher:list
URxvt*keysym.F4: perl:cwd-spawn
URxvt*keysym.Mod4-u: perl:matcher:list
URxvt*keysym.Mod4-o: perl:cwd-spawn

URxvt.keysym.Mod4-Escape: perl:keyboard-select:activate
URxvt.keysym.Mod4-s: perl:keyboard-select:search

! URxvt*termName: rxvt-unicode
/* URxvt*termName: rxvt */
URxvt*termName: rxvt-unicode-256color

